{{#form-field propertyName
    control="one-way-checkbox"
    tagName="li"
    object=object
    formName=formName
    update=update
    as |fc|
}}
  {{#fc.label}}
    {{fc.control (one-way-select/contains (get object propertyName) option)
        disabled=disabled
        dir=dir
        lang=lang
        value=option
    }}
    {{labelText}}
  {{/fc.label}}
{{/form-field}}
