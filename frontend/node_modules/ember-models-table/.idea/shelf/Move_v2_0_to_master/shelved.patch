+    {{mt.table}}
+    {{#mt.footer as |footer|}}
+      {{#footer.summary as |s|}}
+        {{s.firstIndex}}:{{s.lastIndex}}/{{s.recordsCount}}
+      {{/footer.summary}}
+      {{footer.pagination-simple}}
+    {{/mt.footer}}
+  {{/models-table}}`);
